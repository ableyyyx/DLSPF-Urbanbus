{
  "top_k": 20,

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.003,
  "weight_decay": 1e-4,
  "lr_decay": true,
  "lr_scheduler": "CosineAnnealingLR",
  "lr_T_max": 50,
  "lr_eta_min": 1e-5,
  "clip_grad_norm": true,
  "max_grad_norm": 5.0,
  "use_early_stop": true,
  "patience": 30

}
{
  "route_specific_dim": 64,
  "n_heads": 4,
  "contrastive_weight": 0.2,

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 50,
  "learner": "adam",
  "learning_rate": 0.003,
  "weight_decay": 1e-4,
  "lr_decay": false,
  "clip_grad_norm": false,
  "max_grad_norm": 5.0,
  "use_early_stop": true,
  "patience": 30

}